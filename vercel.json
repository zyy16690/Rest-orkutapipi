{
  "version": 2,
  "builds": [
    {
      "src": "api/qris.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/api/(.*)", "dest": "/api/qris.js" }
  ]
}